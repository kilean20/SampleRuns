dpt=2e-06;
dx=1e-11;
dy=1e-11;

beam, particle=electron, radiate=false, energy=0.150511006, npart= 1;
option,-echo;
call file="iota.seq";
option,echo;

use,period=iota;
savebeta, sequence=iota;

twiss,file;

ptc_create_universe;
ptc_create_layout,model=1,method=2,nst=1000,exact;
ptc_align;
ptc_start, x=dx, y=dy, pt=dpt;
       
ptc_track,icase=5, dump,
       //element_by_element,   
       turns=8191, ffile=1;

ptc_track_end; 
ptc_end;

stop;
